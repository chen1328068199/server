<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stan.server.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stan.server.bean.Menu">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="name" property="name" />
        <result column="redirect" property="redirect" />
        <result column="component" property="component" />
        <result column="hide_in_menu" property="hideInMenu" />
        <result column="parent_id" property="parentId" />
        <result column="children_id" property="childrenId" />
        <result column="sort" property="sort" />
        <result column="remark" property="remark" />
        <result column="icon" property="icon" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, `name`, redirect, cmponent, hide_in_menu, parent_id, children_id, sort, remark, icon
    </sql>

</mapper>
